$TTL 604800
@   IN  SOA my-angular.test. admin.my-angular.test. (
                           1     ; Serial
                      604800     ; Refresh
                       86400     ; Retry
                     2419200     ; Expire
                      604800 )   ; Negative Cache TTL
;
@    IN  NS  ns.my-angular.test.
@    IN  A   172.30.0.3   ; This is the IP of the Angular container
ns   IN  A   172.30.0.2   ; This is the IP of the Bind9 container
www  IN  A   172.30.0.3   ; www subdomain points to Angular container
